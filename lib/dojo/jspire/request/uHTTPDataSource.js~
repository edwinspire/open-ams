define("jspire/request/uHTTPDataSource",["dojo/_base/declare", 'dojo/request'],function(_1, _2){

var r = function(){

};


r.prototype = {

data_: {},
url: "/uxsql/default_query.uxsl",

load: function(idrow){
var t = this;
   _2.post(this.url, {
		data: {idrow: idrow},
            handleAs: "json"
        }).then(
                function(response){
               		t.data_ = response;
               		console.log(t.data_);
                },
                function(error){
                    // Display the error returned
console.log(error);
                }
            );


return "";
},

save: function(){

return "";
},

delete: function(){

return "";
}

};

return r;
});
